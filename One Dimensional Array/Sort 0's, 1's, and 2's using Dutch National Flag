#include <stdio.h>
int main()
{
	int n,temp=0;
	scanf("%d",&n);
	int arr[n];
	for(int i=0; i<n; i++) {
		scanf("%d",&arr[i]);
	}
	int low=0,mid=0,high=n-1;
	while(mid<=high){
	  if(arr[mid]==0){
	    temp=arr[low];
	    arr[low]=arr[mid];
	    arr[mid]=temp;
	    low++;
	    mid++;}
	    else if(arr[mid]==1){
	      mid++;
	    }
	    else{
	      temp=arr[high];
	      arr[high]=arr[mid];
	      arr[mid]=temp;
	      high--;
	    }
	}
	   for(int i=0; i<n; i++) {
		printf("%d ",arr[i]);}
} 
